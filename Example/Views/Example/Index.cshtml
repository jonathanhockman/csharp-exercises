@model List<Example.Models.Cheese>;

<h2>Yo Cheese List</h2>

@if (Model.Count == 0)
{
    <h4>No cheeses yet</h4>
}
else
{
    <ul>
        @foreach (var cheese in Model)
    {
        <li>
            <p>
                <strong>@cheese.Name</strong>
                @cheese.Description
                @cheese.Type
            </p>
        </li>
}
    </ul>
    
}
